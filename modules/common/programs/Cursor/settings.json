{
  "workbench.colorTheme": "Catppuccin Frapp√©",
  "workbench.iconTheme": "catppuccin-frappe",
  "window.zoomLevel": 0.0,
  // "window.titleBarStyle": "native",
  // "window.menuBarVisibility": "hidden",
  "workbench.activityBar.orientation": "vertical",
  "editor.semanticTokenColorCustomizations": {
    "enabled": true
  },
  "editor.fontFamily": "'JetBrainsMono Nerd Font', 'Courier New', monospace",
  "editor.fontSize": 12.0,
  "catppuccin.workbenchMode": "flat",
  "catppuccin.italicComments": false,
  "catppuccin.syncWithIconPack": true,
  "editor.fontWeight": "600",
  "editor.fontLigatures": true,
  "editor.inlayHints.enabled": "onUnlessPressed",
  "editor.inlayHints.fontFamily": "Fira Code",
  "editor.inlayHints.fontSize": 11,
  "editor.inlayHints.padding": false,
  "editor.codeLensFontFamily": "InputMono Nerd Font",
  "editor.codeLensFontSize": 12,
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontWeight": "600",
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 1,
  "editor.smoothScrolling": true,
  "editor.minimap.maxColumn": 110,
  "editor.minimap.showSlider": "always",
  "editor.minimap.scale": 1,
  "editor.inlineSuggest.enabled": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.indentation": false,
  "editor.indentSize": 2,
  "files.insertFinalNewline": true,
  "git.untrackedChanges": "separate",
  // "editor.find.seedSearchStringFromSelection": "always",
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": true,
  "gitlens.codeLens.scopes": ["document"],
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.excludeDirty": true,
  "workbench.editor.limit.value": 5,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "cursor.cpp.enablePartialAccepts": true,
  "cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],
  "cursor.cmdk.autoSelect": true,
  "cursor.cmdk.useThemedDiffBackground": true,
  "cursor-retrieval.canAttemptGithubLogin": true,
  "cursor.general.enableShadowWorkspace": false,
  "explorer.confirmDelete": false,
  "rust-analyzer.server.path": "rust-analyzer",
  "rust-analyzer.inlayHints.maxLength": 40,
  "rust-analyzer.inlayHints.renderColons": true,
  "rust-analyzer.testExplorer": true,
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.cargo.targetDir": true,
  "vscode-neovim.revealCursorScrollLine": true,
  "vscode-neovim.neovimViewportHeightExtend": 2,
  "shellcheck.logLevel": "warn",
  "git.autofetch": true,
  "editorconfig.generateAuto": false,
  "git.confirmSync": false,
  "git.rebaseWhenSync": true,
  "git.openRepositoryInParentFolders": "always",
  "scm.diffDecorationsGutterPattern": {
    "added": true,
    "modified": true
  },
  "testExplorer.hideWhen": "noAdapters",
  "testExplorer.useNativeTesting": true,
  "gitlens.views.commitDetails.files.layout": "tree",
  // "nvim-ui.nvimColorCustomizationKeys": ["statusBar.background"],
  // "nvim-ui.nvimColorNormal": null,
  // "nvim-ui.nvimColorInsert": "#2F8F2F",
  // "nvim-ui.nvimColorVisual": "#673AB7",
  // "nvim-ui.nvimColorReplace": "#C06700",
  "nix.serverPath": "nixd",
  "nix.enableLanguageServer": true,
  "nix.hiddenLanguageServerErrors": [
    "textDocument/definition",
    "textDocument/documentSymbol",
    "textDocument/formatting"
  ],
  "nix.formatterPath": "nixfmt",
  "nix.serverSettings": {
    "nixd": {
      "formatting": {
        "command": ["nixfmt", "-w", "80"]
      },
      "options": {
        "nixpkgs": {
          // "expr": "import (builtins.getFlake (toString <self>)).inputs.nixpkgs {}"
          "expr": "import (builtins.getFlake \"/home/jeff/.config/nixpkgs/\").inputs.nixpkgs {}"
        },
        "nixos": {
          "expr": "(builtins.getFlake \"/home/jeff/.config/nixpkgs/\").nixosConfigurations.jeff-nixos.options"
        },
        "home-manager": {
          "expr": "(builtins.getFlake \"/home/jeff/.config/nixpkgs/\").homeConfigurations.default.options"
        }
      }
    },
    "nil": {
      "formatting": {
        "command": ["/etc/profiles/per-user/jeff/bin/nixfmt", "-w", "80"]
      },
      "diagnostics": {
        "ignored": ["unused_binding"]
      },
      "nix": {
        "binary": "/run/current-system/sw/bin/nix",
        "flake": {
          "autoArchive": true,
          "autoEvalInputs": false,
          "nixpkgsInputName": "nixpkgs"
        }
      }
    }
  },
  "vscode-neovim.ctrlKeysForInsertMode": [],
  "vscode-neovim.ctrlKeysForNormalMode": [
    "a",
    "d",
    "e",
    "l",
    "r",
    "w",
    "backspace",
    "delete"
  ],
  "prettier.useEditorConfig": true,
  "yaml.validate": false,
  "evenBetterToml.schema.enabled": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[clojure]": {
    // "editor.defaultFormatter": "rflagreca.vscode-clj-zprint"
    "editor.defaultFormatter": "betterthantomorrow.calva"
  },
  "calva.paredit.killAlsoCutsToClipboard": true,
  "calva.highlight.ignoredFormStyle": {
    "color": "#808080"
  },
  "calva.highlight.misplacedBracketStyle": {
    "backgroundColor": "#ff6a40",
    "color": "#fbfbfb"
  },
  "calva.fmt.newIndentEngine": false,
  "calva.fmt.configPath": ".cljfmt.edn",
  // "calva.prettyPrintingOptions": {
  //   // "printEngine": "calva"
  //   // "enabled": true
  //   // "width": 100,
  //   // "maxLength": 50
  // },
  "calva.paredit.defaultKeyMap": "strict",
  "calva.enableClojureLspOnStart": "when-file-opened-use-furthest-project",
  "editor.tokenColorCustomizations": {
    "comments": {
      "foreground": "#989898"
    }
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cursor.diffs.useCharacterLevelDiffs": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "python.poetryPath": "poetry",
  "vsicons.dontShowNewVersionMessage": true,
  "files.associations": {
    "mbsyncrc": "plaintext"
  },
  "update.releaseTrack": "prerelease",
  "redhat.telemetry.enabled": true,
  "calva.autoOpenREPLWindow": true,
  "calva.evaluationSendCodeToOutputWindow": true,
  // "calva.referencesCodeLens.enabled": true,
  "calva.autoConnectRepl": true
}
