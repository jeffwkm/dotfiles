[
  {
    "key": "meta+ctrl+shift+p",
    "command": "workbench.action.tasks.toggleProblems"
  },
  {
    "key": "alt+.",
    "command": "editor.action.goToDeclaration"
  },
  {
    "key": "alt+shift+.",
    "command": "editor.action.goToReferences"
  },
  {
    "key": "alt+,",
    "command": "workbench.action.navigateBack"
  },
  {
    "key": "alt+shift+,",
    "command": "workbench.action.navigateForward"
  },
  {
    "key": "meta+h",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "meta+l",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "meta+v",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "meta+b",
    "command": "workbench.action.toggleAuxiliaryBar"
  },
  {
    "key": "meta+shift+h",
    "command": "workbench.action.moveEditorLeftInGroup"
  },
  {
    "key": "meta+shift+l",
    "command": "workbench.action.moveEditorRightInGroup"
  },
  {
    "key": "meta+ctrl+h",
    "command": "workbench.action.focusLeftGroup"
  },
  {
    "key": "meta+ctrl+l",
    "command": "workbench.action.focusRightGroup"
  },
  {
    "key": "ctrl+w w",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+w q",
    "command": "workbench.action.closeGroup"
  },
  {
    "key": "meta+ctrl+shift+h",
    "command": "workbench.action.moveEditorToLeftGroup"
  },
  {
    "key": "meta+ctrl+shift+l",
    "command": "workbench.action.moveEditorToRightGroup"
  },
  {
    "key": "meta+ctrl+shift+j",
    "command": "workbench.action.moveEditorToBelowGroup"
  },
  {
    "key": "meta+ctrl+shift+k",
    "command": "workbench.action.moveEditorToAboveGroup"
  },
  {
    "key": "ctrl+shift+p",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "ctrl+s",
    "command": "workbench.action.files.save"
  },
  {
    "key": "ctrl+shift+s",
    "command": "workbench.action.files.saveAll"
  },
  {
    "key": "ctrl+shift+r",
    "command": "workbench.action.reloadWindow"
  },
  {
    "key": "ctrl+n",
    "when": "inQuickOpen",
    "command": "-workbench.action.quickOpenSelectNext"
  },
  {
    "key": "ctrl+p",
    "when": "inQuickOpen",
    "command": "-workbench.action.quickOpenSelectPrevious"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+n",
    "command": "-selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+j",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+p",
    "command": "-selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+k",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+e",
    "command": "workbench.action.focusActiveEditorGroup"
  },
  {
    "key": "ctrl+'",
    "command": "workbench.action.focusNextPart"
  },
  {
    "key": "ctrl+;",
    "command": "workbench.action.focusPreviousPart"
  },
  {
    "key": "ctrl+left",
    "command": "workbench.action.navigateLeft"
  },
  {
    "key": "ctrl+j",
    "command": "-workbench.action.togglePanel"
  },
  // Focus next search result when search results view focused
  {
    "key": "ctrl+j",
    "command": "search.action.focusNextSearchResult",
    "when": "searchViewletVisible"
  },
  // Focus previous search result when in search view or search results view
  {
    "key": "ctrl+k",
    "command": "search.action.focusPreviousSearchResult",
    "when": "searchViewletVisible"
  },
  {
    "key": "ctrl+shift+t",
    "command": "-workbench.action.reopenClosedEditor"
  },
  {
    "key": "j",
    "command": "list.focusDown",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "k",
    "command": "list.focusUp",
    "when": "listFocus && !inputFocus"
  }
]
